<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianhao.notesonline.dao.TagDao">
    <select id="findById" resultType="com.qianhao.notesonline.beans.Tag" parameterType="int">
        select *
        from tags
        where t_id=#{id}
    </select>


</mapper>
